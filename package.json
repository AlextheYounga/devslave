{
    "name": "devslave",
    "version": "1.0.0",
    "description": "A minion for coding",
    "main": "dist/index.js",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "dev": "tsx src/cli.ts",
        "docker:start": "./start.sh",
        "build": "tsc -p .",
        "server": "tsx src/server.ts",
        "server:dev": "tsx watch src/server.ts",
        "db:migrate": "prisma migrate dev",
        "db:migrate:test": "npx dotenv -e .env.test -- npx prisma migrate dev",
        "db:reset:test": "npx dotenv -e .env.test -- npx prisma migrate reset",
        "format": "npx prettier --write --plugin=prettier-plugin-sh \"**/*.{js,jsx,ts,tsx,css,scss,json,md,sh}\"",
        "test": "jest"
    },
    "author": "Alex Younger, Trey Goff",
    "license": "ISC",
    "dependencies": {
        "@openai/codex": "github:openai/codex",
        "@paralleldrive/cuid2": "^3.1.0",
        "@prisma/client": "^6.15.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.2",
        "dotenv-cli": "^10.0.0",
        "express": "^5.1.0",
        "gray-matter": "^4.0.3",
        "inquirer": "^12.9.4",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.27.1",
        "@faker-js/faker": "^10.0.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.3.1",
        "@types/supertest": "^2.0.16",
        "jest": "^30.1.3",
        "prettier": "^3.6.2",
        "prettier-plugin-sh": "^0.18.0",
        "prisma": "^6.15.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.4.1",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2"
    }
}
