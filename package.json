{
  "name": "devslave",
  "version": "1.0.0",
  "description": "A minion for coding",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsc -p .",
    "server": "tsx src/server.ts",
    "server:dev": "tsx watch src/server.ts",
    "db:migrate": "prisma migrate dev",
    "db:migrate:test": "npx dotenv -e .env.test -- npx prisma migrate dev",
    "db:reset:test": "npx dotenv -e .env.test -- npx prisma migrate reset",
    "test": "jest --silent"
  },
  "author": "Alex Younger, Trey Goff",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "express": "^5.1.0",
    "inquirer": "^12.9.4",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "jest": "^30.1.3",
    "prisma": "^6.15.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
